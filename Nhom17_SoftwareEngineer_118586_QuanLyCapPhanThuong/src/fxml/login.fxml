<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" stylesheets="/css/Login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.guicontroller.LoginGUIController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="901.0">
         <image>
            <Image url="icons/background.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="101.0" layoutY="50.0" prefHeight="600.0" prefWidth="700.0" style="-fx-background-color: #ffffff;">
         <children>
            <ImageView fitHeight="244.0" fitWidth="271.0" layoutX="52.0" layoutY="156.0">
               <image>
                  <Image url="icons/img-01.png" />
               </image>
            </ImageView>
            <Text layoutX="470.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="LOGIN">
               <font>
                  <Font name="Tahoma Bold" size="36.0" />
               </font>
            </Text>
            <TextField fx:id="username" layoutX="406.0" layoutY="205.0" prefHeight="45.0" prefWidth="250.0" promptText="User Name" style="-fx-border-color: #a2a2a2; -fx-background-color: #ffffff; -fx-border-width: 0px 0px 2px 0px;">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <PasswordField fx:id="password" layoutX="406.0" layoutY="278.0" prefHeight="45.0" prefWidth="250.0" promptText="Password" style="-fx-background-color: #ffffff; -fx-border-color: #a2a2a2; -fx-border-width: 0px 0px 2px 0px;">
               <font>
                  <Font size="20.0" />
               </font>
            </PasswordField>
            <Button layoutX="405.0" layoutY="378.0" mnemonicParsing="false" onAction="#login" text="Enter" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
